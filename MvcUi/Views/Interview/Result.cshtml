
@model Core.Models.InterviewExam.ResultModel
@{
    ViewData["Title"] = "Result";
}
<link href="~/css/result.css" rel="stylesheet" />

<div class="card">
      <h4 class="card-header">Exam Result</h4>
      <div class="card-body">
          <div class="font-weight-bold">Your Exam is Submited ... </div>
          <h6>Your Score : <span class="font-italic text-info">@Model.Score</span></h6>
      </div>
    <a asp-controller="Home" asp-action="Index" class="btn btn-primary">Home</a>
</div>



